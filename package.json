{
  "name": "webpack",
  "version": "4.35.2",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.less"
  ],
  "description": "webpack basic configuration",
  "scripts": {
    "dev-build": "webapck --config ./build/webpack.dev.js ",
    "dev": "cross-env NODE_ENV=development webpack-dev-server  --config ./build/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config  ./build/webpack.prod.js ",
    "build:dll": "cross-env NODE_ENV=production webpack --config  ./build/webpack.dll.js",
    "build:all": "cross-env NODE_ENV=production  webpack --config  ./build/webpack.prod.js && webpack --config  ./build/webpack.dll.js",
    "start": "node server.js"
  },
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "QM",
  "license": "ISC",
  "dependencies": {
    "antd": "^3.23.6",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-datepicker": "^1.9.0",
    "core-js": "^3.3.2",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.1"
  },
